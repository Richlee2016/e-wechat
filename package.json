{
  "name": "e-wechat",
  "version": "1.0.0",
  "description": "typescript egg wechat",
  "private": true,
  "egg": {
    "typescript": true,
    "require": [
      "egg-ts-helper/register"
    ]
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-e-wechat",
    "stop": "egg-scripts stop --title=egg-server-e-wechat",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "tslint --project . -c tslint.json",
    "clean": "ets clean"
  },
  "dependencies": {
    "@types/joi": "^14.0.1",
    "@types/lodash": "^4.14.119",
    "egg": "^2.6.1",
    "egg-mongoose": "^3.1.1",
    "egg-redis": "^2.3.0",
    "egg-scripts": "^2.6.0",
    "egg-session": "^3.1.0",
    "egg-view-nunjucks": "^2.2.0",
    "joi": "^14.3.1",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "@types/supertest": "^2.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-ci": "^1.8.0",
    "egg-bin": "^4.6.2",
    "egg-mock": "^3.16.0",
    "egg-ts-helper": "^1.11.0",
    "tslib": "^1.9.0",
    "tslint": "^4.0.0",
    "typescript": "^3.0.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "rich lee",
  "license": "MIT"
}
