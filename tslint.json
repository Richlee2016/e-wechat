{
  "extends": [
    "tslint-eslint-rules",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "rules": {
    "prettier": true,
    "no-console": false,
    "no-unused-variable": true,
    "no-unused-expression": [true, "allow-new", "allow-fast-null-checks"],
    "no-trailing-whitespace": true
  }
}
